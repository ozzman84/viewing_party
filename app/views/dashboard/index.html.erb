<div class='test' id='username'>
  <h1><%= "Welcome #{@user.username}" %></h1>
</div>

<div class='test' id='discover-movies'>
  <h1><%= link_to 'Discover New Movies', movies_path %></h1>
</div>

<div class='test' id='friends'>

  <%= render "new_friend_form" %>

  <h1>Friends</h1>
  <% @user.get_friends.each do |friend| %>
    <%= friend.username %>
  <% end %>
</div>

<div class='container' id='viewing-party'>
  <h1>Viewing Parties</h1>
  <% binding.pry %>
  <% @user.events.each do |event| %>
    <%= event.title %>
    <%= event.starttime %>
    <%= event.duration %>
  <% end %>
</div>
