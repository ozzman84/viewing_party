<h1>Welcome Friend</h1>

<section id="sign-in">
    <%= form_with url: sign_in_path, local: true do |f| %>
        <%= f.label :email %><br/>
        <%= f.text_field :email %><br/>
        <%= f.label :password %><br/>
        <%= f.password_field :password %><br/>
        <%= f.submit "Log In" %>
    <% end %>
</section>

<%= link_to "Sign Up", sign_up_path %>
